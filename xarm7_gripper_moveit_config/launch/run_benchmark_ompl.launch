<launch>

  <!-- Load URDF -->
  <include file="$(find xarm7_gripper_moveit_config)/launch/planning_context.launch">
    <arg name="load_robot_description" value="true"/>
  </include>

  <!-- Start the database -->
  <include file="$(find xarm7_gripper_moveit_config)/launch/warehouse.launch">
    <arg name="moveit_warehouse_database_path" value="$(find xarm7_moveit_config)/default_warehouse_mongo_db"/>
  </include>

  <!-- Start Benchmark Executable -->
  <node name="$(anon moveit_benchmark)" pkg="moveit_ros_benchmarks" type="moveit_run_benchmark" respawn="false" output="screen">
    <rosparam command="load" file="$(find xarm7_gripper_moveit_config)/config/kinematics.yaml"/>
    <rosparam command="load" file="$(find xarm7_gripper_moveit_config)/config/ompl_planning.yaml"/>
    <rosparam command="load" file="$(find xarm7_gripper_moveit_config)/config/benchmark_config.yaml"/>
  </node>

</launch>
